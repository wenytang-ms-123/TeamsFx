{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.9/MicrosoftTeams.schema.json",
  "manifestVersion": "1.9",
  "version": "1.0.0",
  "id": "{appid}",
  "packageName": "com.microsoft.teams.extension",
  "developer": {
    "name": "Teams App, Inc.",
    "websiteUrl": "{baseUrl}",
    "privacyUrl": "{baseUrl}/privacy",
    "termsOfUseUrl": "{baseUrl}/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "manifest-validation",
    "full": ""
  },
  "description": {
    "short": "Short description for Personal App.",
    "full": "Full description of Personal App."
  },
  "accentColor": "#FFFFFF",
  "staticTabs": [
    {
      "entityId": "index",
      "name": "Personal Tab",
      "contentUrl": "{baseUrl}/tab",
      "websiteUrl": "{baseUrl}/tab",
      "scopes": ["personal"]
    }
  ],
  "permissions": ["identity", "messageTeamMembers"],
  "validDomains": []
}
